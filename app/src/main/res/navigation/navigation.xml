<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">
    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if3135.mobpro1.ui.hitung.HitungFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_hitung">
        <action
            android:id="@+id/action_hitungFragment_to_saranFragment"
            app:destination="@id/saranFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_historiFragment"
            app:destination="@id/historiFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_daftarFragment"
            app:destination="@id/daftarFragment" />
    </fragment>
    <fragment
        android:id="@+id/saranFragment"
        android:name="org.d3if3135.mobpro1.ui.SaranFragment"
        android:label="Saran"
        tools:layout="@layout/fragment_saran">
        <argument
            android:name="kategori"
            app:argType="org.d3if3135.mobpro1.model.KategoriNilai" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if3135.mobpro1.ui.AboutFragment"
        android:label="Tentang Aplikasi"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if3135.mobpro1.ui.histori.HistoriFragment"
        android:label="Histori"
        tools:layout="@layout/fragment_histori" />
    <fragment
        android:id="@+id/daftarFragment"
        android:name="org.d3if3135.mobpro1.ui.daftar.DaftarFragment"
        android:label="Daftar Orang Terpintar" />
</navigation>